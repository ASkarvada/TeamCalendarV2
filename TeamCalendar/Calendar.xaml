        <TextBlock x:Name="pondeli" HorizontalAlignment="Left" Height="20" Margin="10,20,0,0" TextWrapping="Wrap" Text="Pondělí" VerticalAlignment="Top" Width="93" Grid.Row="1" FontSize="18" TextAlignment="Center"/>
        <TextBlock x:Name="utery" HorizontalAlignment="Left" Height="30" Margin="10,20,0,0" TextWrapping="Wrap" Text="Úterý" VerticalAlignment="Top" Width="93" Grid.Row="1" Grid.Column="1" FontSize="18" TextAlignment="Center"/>
